{"cells":[{"cell_type":"code","source":"import pandas as pd\nimport os\nimport io\n\n# --- Data for Equities ex-ETFs (from user-provided text) ---\nequities_ex_etf_data = \"\"\"\nBusiness Date,Active Lendable Quantity,On Loan Value\n7/10/2023,9.31E+11,9.57E+11\n7/11/2023,9.31E+11,9.57E+11\n7/12/2023,9.32E+11,9.71E+11\n7/13/2023,9.33E+11,9.83E+11\n7/14/2023,9.3E+11,9.82E+11\n7/17/2023,9.3E+11,9.7E+11\n7/18/2023,9.27E+11,9.73E+11\n7/19/2023,9.26E+11,9.82E+11\n7/20/2023,9.27E+11,9.86E+11\n7/21/2023,9.25E+11,9.91E+11\n7/24/2023,9.27E+11,9.93E+11\n7/25/2023,9.26E+11,1.02E+12\n7/26/2023,9.25E+11,1.02E+12\n7/27/2023,9.26E+11,1E+12\n7/28/2023,9.27E+11,1.01E+12\n7/31/2023,9.25E+11,9.97E+11\n1/1/2025,9.61E+11,1E+12\n1/2/2025,9.6E+11,9.98E+11\n1/3/2025,9.62E+11,1.01E+12\n6/30/2025,1.01E+12,1.23E+12\n\"\"\"\n\n# --- Configuration ---\n# This dictionary maps the original CSV filenames to the clean tab names.\nfiles_to_consolidate = {\n    'all_etfs_jun_30_2025_2025-07-10_bsheehan_adhoc.csv': 'Global_ETFs_June_2025',\n    'all_atfs_jan1_2025-07-10_bsheehan_adhoc.csv': 'Global_ETFs_Jan_2025',\n    'all_etf_july_30_2023_2025-07-10_bsheehan_adhoc.csv': 'Global_ETFs_July_2023',\n    'northam_etfs_jun2025_2025-07-10_bsheehan_adhoc.csv': 'NorthAm_ETFs_June_2025',\n    'jan2025northametfs_2025-07-10_bsheehan_adhoc.csv': 'NorthAm_ETFs_Jan_2025',\n    'all_northam_etf_jul2023_bsheehan_adhoc.csv': 'NorthAm_ETFs_July_2023',\n    'on loan act len qty etfs.csv': 'OnLoan_ActiveLend_Qty_ETFs',\n    'etf vs ex etf.csv': 'AvgFee_ETF_vs_ex-ETF'\n}\n\noutput_filename = 'EquiLend_ETF_Consolidated_Data.xlsx'\n\n# --- Main Script ---\ndef create_consolidated_spreadsheet():\n    \"\"\"\n    Reads multiple CSV files and a text data block, then consolidates them\n    into a single Excel spreadsheet with each source on a separate tab.\n    \"\"\"\n    try:\n        with pd.ExcelWriter(output_filename, engine='openpyxl') as writer:\n            print(f\"Creating Excel file: {output_filename}\")\n\n            # Loop through the dictionary of CSV files\n            for csv_file, sheet_name in files_to_consolidate.items():\n                if os.path.exists(csv_file):\n                    print(f\"  - Reading '{csv_file}'...\")\n                    df = pd.read_csv(csv_file)\n                    print(f\"  - Writing to sheet: '{sheet_name}'...\")\n                    df.to_excel(writer, sheet_name=sheet_name, index=False)\n                else:\n                    print(f\"  - WARNING: File not found - '{csv_file}'. Skipping.\")\n\n            # Process and add the equities ex-ETF time series data\n            print(\"  - Processing Equities ex-ETF data...\")\n            df_ex_etf = pd.read_csv(io.StringIO(equities_ex_etf_data))\n            df_ex_etf.dropna(inplace=True)\n            for col in ['Active Lendable Quantity', 'On Loan Value']:\n                df_ex_etf[col] = pd.to_numeric(df_ex_etf[col], errors='coerce')\n            df_ex_etf['Business Date'] = pd.to_datetime(df_ex_etf['Business Date'], errors='coerce')\n            df_ex_etf.dropna(inplace=True)\n            print(\"  - Writing to sheet: 'Equities_ex-ETFs_TimeSeries'...\")\n            df_ex_etf.to_excel(writer, sheet_name='Equities_ex-ETFs_TimeSeries', index=False)\n\n        print(f\"\\nSuccessfully created '{output_filename}'\")\n\n    except Exception as e:\n        print(f\"\\nAn error occurred: {e}\")\n\n# --- Execute the function ---\nif __name__ == \"__main__\":\n    create_consolidated_spreadsheet()","outputs":[],"execution_count":null,"metadata":{}}],"metadata":{"colab":{"from_bard":true},"kernelspec":{"display_name":"Python 3","name":"python3"}},"nbformat":4,"nbformat_minor":0}